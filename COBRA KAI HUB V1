
local VLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/hakaiofc/cobrakai/main/COBRA%20KAI%20HUB%20V1"))()

MAINTTL = "COBRA KAI HUB V1"

local win = VLib:Window("COBRA KAI V1 ", Color3.fromRGB(196, 40, 28))

local ss = win:Tab("MENU")
local sss = win:Tab("CREDITS")

--ANTI AFK 
pcall(function()
    local VirtualUser=game:service'VirtualUser'
    game:service'Players'.LocalPlayer.Idled:connect(function()
    warn("anti-afk")
    VirtualUser:CaptureController()
    VirtualUser:ClickButton2(Vector2.new())
       end)
    end) 

--GLOBALS 
LP = game.Players.LocalPlayer
VIM = game:GetService("VirtualInputManager")

--MAIN SCRIPT

ss:Toggle("Autofarm ALL",function(t)
    farm = t  _G.KVN_OFC = true 
while _G.KVN_OFC do wait(0.48)if _G.KVN_OFC == false then return end
      pcall(function()
game:GetService("ReplicatedStorage").Remotes.Dumbbell:FireServer()

game:GetService("ReplicatedStorage").Remotes.Pushup:FireServer()

game:GetService("ReplicatedStorage").Remotes.Situp:FireServer()

     end)  
  end
end) 



sss:Label("MADE BY ALBINO")
